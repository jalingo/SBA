source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.3'   # <- 10.3 minimum requirement, can be more recent...
use_frameworks!         # <- MagicCloud is a swift framework, ensure this is present.

target 'Small Biz Advisor' do
    pod 'MagicCloud', '~> 2.9.8'  # <- Be sure to use the current version.

  target 'Small Biz AdvisorTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Small Biz AdvisorUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
